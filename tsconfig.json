{
    "compilerOptions": {
    "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "react",
        "moduleResolution": "node",
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "sourceMap": true,
        "strict": true,
        "resolveJsonModule": true,
    "target": "es6",
    },
    "include": ["src"],
}